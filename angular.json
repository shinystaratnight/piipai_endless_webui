    "r3sourcer": {
      "projectType": "application",
      "schematics": {
        "@nrwl/workspace:component": {
          "style": "scss"
        }
      },
      "root": "apps/r3sourcer",
      "sourceRoot": "apps/r3sourcer/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "apps/r3sourcer/src/index.html",
            "main": "apps/r3sourcer/src/main.ts",
            "polyfills": "apps/r3sourcer/src/polyfills.ts",
            "tsConfig": "apps/r3sourcer/tsconfig.app.json",
            "assets": [
              "apps/r3sourcer/src/favicon.ico",
              "apps/r3sourcer/src/assets"
            ],
            "styles": [
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/jtsage-datebox-bootstrap4/jtsage-datebox.css",
              "node_modules/nouislider/distribute/nouislider.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/intl-tel-input/build/css/intlTelInput.css",
              "apps/r3sourcer/src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/jquery-ui-dist/jquery-ui.js",
              "node_modules/jtsage-datebox-bootstrap4/jtsage-datebox.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/r3sourcer/src/environments/environment.ts",
                  "with": "apps/r3sourcer/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "stage": {
              "fileReplacements": [
                {
                  "replace": "apps/r3sourcer/src/environments/environment.ts",
                  "with": "apps/r3sourcer/src/environments/environment.stage.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "r3sourcer:build",
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "r3sourcer:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "r3sourcer:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/r3sourcer/tsconfig.app.json",
              "apps/r3sourcer/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/r3sourcer/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/r3sourcer/jest.config.js",
            "tsConfig": "apps/r3sourcer/tsconfig.spec.json",
            "setupFile": "apps/r3sourcer/src/test-setup.ts"
          }
        }
      }
    },
